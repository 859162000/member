			<field src="IS_SYNC_BALANCE" dest="IS_SYNC_BALANCE" />
			 <field src="ISDELETE" dest="ISDELETE"  />
			<field src="CREATE_BY" dest="CREATE_BY" />
			<field src="CREATE_DATE"  dest="CREATE_DATE" type="timestamp"/> 
			<field src="UPDATE_BY" dest="UPDATE_BY" />
