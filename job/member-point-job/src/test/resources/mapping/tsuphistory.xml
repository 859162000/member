			<field src="CINEMA_INNER_CODE" dest="CINEMA_INNER_CODE" />
			<field src="POINT_EXT_RULE_ID" dest="POINT_EXT_RULE_ID" />
			<field src="TRANS_TYPE" dest="TRANS_TYPE" />
			<field src="T_TRANS_ORDER_ID" dest="T_TRANS_ORDER_ID"  />
