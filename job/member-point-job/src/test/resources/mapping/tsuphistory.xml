			<field src="POINT_SYS" dest="POINT_SYS" />
			<field src="POINT_TRANS_TYPE" dest="POINT_TRANS_TYPE"  />
			<field src="POINT_TRANS_CODE" dest="POINT_TRANS_CODE" />
			<field src="POINT_TRANS_CODE_WEB" dest="POINT_TRANS_CODE_WEB"  />
